str = ['разработка', 'сокет', 'декоратор']

for line in str:
    print('тип переменной: {}\n'.format(type(line)))
    print('содержание переменной - {}\n'.format(line))

str = [u'\u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430',u'\u0441\u043e\u043a\u0435\u0442', u'\u0434\u0435\u043a\u043e\u0440\u0430\u0442\u043e\u0440']

for line in str:
    print('тип переменной: {}\n'.format(type(line)))
    print('содержание переменной - {}\n'.format(line))
